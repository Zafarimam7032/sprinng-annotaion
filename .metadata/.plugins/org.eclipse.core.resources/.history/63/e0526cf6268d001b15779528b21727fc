package com.grwts.depricated;

public class DepricatedImplimentation {
	private int number1=1000;
	private int number2=20;

	public int add() {
		return number1 + number2;
	}

	public int sub() {
		return number1 - number2;
	}

	public int div() {
		return number1 / number2;
	}

	public int mul() {
		return number1 * number2;
	}
}
